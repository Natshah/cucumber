{
  "name": "natshah/cucumber",
  "description": "Cucumber Management System",
  "type": "drupal-profile",
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Rajab Natshah",
      "homepage": "https://www.drupal.org/u/rajabnatshah",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/cucumber",
    "source": "http://cgit.drupalcode.org/cucumber"
  },
  "repositories": [
      {
        "type": "composer",
        "url": "https://packages.drupal.org/8"
      }
  ],
  "config": {
        "bin-dir": "bin/",
        "sort-packages": true,
        "secure-http": false
  },
  "scripts": {
    "post-install-cmd": [
      "@composer drupal-scaffold"
    ],
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
  },
  "require-dev": {
      "drush/drush": "^8.0.0",
      "drupal/drupal-extension": "^3.2",
      "webflo/drupal-finder": "^0.3.0",
      "webmozart/path-util": "^2.3",
      "jakoch/phantomjs-installer": "1.9.8",
      "behat/behat": "^3.0",
      "drupal/coder": "8.*",
      "behat/mink": "~1.7",
      "behat/mink-goutte-driver": "~1.2",
      "behat/mink-zombie-driver": "@stable",
      "behat/mink-selenium2-driver":  "~1.3",
      "mikey179/vfsStream": "~1.2",
      "phpunit/phpunit": "~4.8",
      "guzzlehttp/guzzle": "6.2.*@stable",
      "symfony/css-selector": "~2.8",
      "squizlabs/php_codesniffer": "2.*",
      "behatch/contexts": "*",
      "emuse/behat-html-formatter": "@stable"
  },
  "require": {
      "cweagans/composer-patches": "^1.6",
      "composer/installers": "^1.2",
      "drupal-composer/drupal-scaffold": "^2.2",
      "drupal/core": "8.3.4",
      "drupal/agnian_material_admin": "1.21",
      "drupal/uikit": "2.9",
      "drupal/uikit_components": "2.1",
      "drupal/ctools": "3.0",
      "drupal/entity": "1.0-alpha4",
      "drupal/config_update": "1.3",
      "drupal/token": "1.0",
      "drupal/page_manager": "4.0-beta2",
      "drupal/panels": "4.1",
      "drupal/ds": "3.1",
      "drupal/field_group": "1.0-rc6",
      "drupal/smart_trim": "1.0",
      "drupal/pathauto": "1.0",
      "drupal/features": "3.5"
  },
  "extra": {
      "branch-alias": {
          "dev-8.x-1.x": "8.1.x-dev"
      },
      "installer-paths": {
          "web/core": ["type:drupal-core"],
          "web/libraries/{$name}": ["type:drupal-library"],
          "web/modules/{$name}": ["type:drupal-module"],
          "web/profiles/{$name}": ["type:drupal-profile"],
          "web/themes/{$name}": ["type:drupal-theme"],
          "drush/{$name}": ["type:drupal-drush"]
      },
      "enable-patching": true
  }
}
