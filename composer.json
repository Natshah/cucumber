{
  "name": "webship/cucumber",
  "description": "Cucumber Management System",
  "type": "drupal-profile",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Webship",
      "homepage": "https://www.drupal.org/webship",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/cucumber",
    "source": "http://cgit.drupalcode.org/cucumber"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "composer/installers": "@stable",
    "cweagans/composer-patches": "@stable",
    "drupal/core-composer-scaffold": "@stable",
    "drupal/core-recommended": "^8.8",
    "drupal/console": "^1.0",
    "drush/drush": "^9.0",
    "drupal/config_update": "@stable",
    "drupal/libraries": "@stable",
    "drupal/admin_toolbar": "@stable",
    "drupal/ctools": "@stable",
    "drupal/entity": "@stable",
    "drupal/token": "@stable",
    "drupal/page_manager": "@stable",
    "drupal/panels": "@stable",
    "drupal/ds": "@stable",
    "drupal/field_group": "@stable",
    "drupal/smart_trim": "@stable",
    "drupal/pathauto": "@stable",
    "drupal/behat_ui": "*",
    "drupal/uikit_admin": "@stable"
  },
  "config": {
    "bin-dir": "bin/",
    "secure-http": false
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      }
    },
    "branch-alias": {
      "dev-8.x-1.x": "8.1.x-dev"
    },
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/libraries/{$name}": ["type:drupal-library"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/profiles/{$name}": ["type:drupal-profile"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
      "web/modules/custom/{$name}": ["type:drupal-custom-module"],
      "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    }
  }
}
